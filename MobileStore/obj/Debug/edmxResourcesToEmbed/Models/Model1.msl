      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Details">
      <EntityTypeMapping TypeName="Model.Detail">
        <MappingFragment StoreEntitySet="Details">
          <ScalarProperty Name="Details_Id" ColumnName="Details Id" />
