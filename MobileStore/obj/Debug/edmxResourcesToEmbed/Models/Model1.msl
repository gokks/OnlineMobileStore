      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Payment_Details">
      <EntityTypeMapping TypeName="Model.Payment_Detail">
        <MappingFragment StoreEntitySet="Payment Details">
          <ScalarProperty Name="Payment_Id" ColumnName="Payment Id" />
